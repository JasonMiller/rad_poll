<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "vote-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :option_id %>
  <%= number_input f, :option_id %>
  <%= error_tag f, :option_id %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
